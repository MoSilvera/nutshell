{
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 0,
        "eqeqeq": 2,
        "no-trailing-spaces": 2,
        "indent": 0,
        "no-mixed-spaces-and-tabs": 1,
        "no-var": 1
    }
}